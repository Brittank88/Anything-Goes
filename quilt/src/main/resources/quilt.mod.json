{
	"schema_version": 1,

	"mixin": [
		"${modId}.mixins.json",
		"${modId}-common.mixins.json"
	],

	"quilt_loader": {
		"group": "${modGroup}",
		"id": "${modId}",
		"version": "${modVersion}",

		"name": "${modName}",
		"description": "${modDescription}",
		"authors": [
			"${modAuthor}"
		],
		"contact": {
			"homepage": "${modPageUrl}",
			"issues": "${modIssueUrl}",
			"sources": "${modPageUrl}"
		},

		"license": "${modLicense}",
		"icon": "${modLogoFile}",

		"intermediate_mappings": "net.fabricmc:intermediary",
		"environment": "*",
		"entrypoints": {
			"init": [
				"${mainEntryPoint}"
			]
		},
		"depends": [
			{
				"id": "java",
				"version": ">=${minJavaVersion}"
			},
			{
				"id": "quilt_loader",
				"version": ">=${minQuiltLoaderVersion}"
			},
			{
				"id": "quilt_base",
				"version": ">=${minQuiltBaseVersion}"
			},
			{
				"id": "minecraft",
				"version": "[${minMinecraftVersion},${nextMinecraftVersion})"
			},
			{
				"id": "architectury",
				"version": ">=${architecturyVersion}"
			},
			{
				"id": "forgeconfigapiport",
				"version": ">=${forgeConfigApiPortVersion}"
			},
			{
				"id": "puzzleslib",
				"version": ">=${puzzleslibVersion}"
			},
			{
				"id": "extensibleenums",
				"version": ">=${extensibleEnumsVersion}"
			}
		]
	},

	"catalogue": {
		"icon" : {
			"image": "${modIconFile}",
			"item": "${modIconItem}"
		}
	},
	"configured": {
		"background": "${configuredBackgroundFile}"
	}
}